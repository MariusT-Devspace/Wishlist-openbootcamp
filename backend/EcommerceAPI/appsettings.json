{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "EcommerceDB": "Server=localhost\\SQLEXPRESS;Database=EcommerceDB;Trusted_Connection=True;TrustServerCertificate=True"
  },
  "JwtKeys": {
    "ValidateIssuerSigningKey": true,
    "IssuerSigningKey": "44A3C153-1B41-4A69-71D3-E4AF46A9D186",
    "ValidateIssuer": true,
    "ValidIssuers": [
      "https://localhost:7201",
      "https://wishlist-api.azurewebsites.net"
    ],
    "ValidateAudience": true,
    "ValidAudience": "https://localhost:4200",
    "RequireExpirationTime": true,
    "ValidateLifeTime": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.LifeTime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "VaultURIString": "https://wishlistapivault.vault.azure.net/"
}


